{% doc %}
  @prompt
    Create an enhanced collapsible accordion section with custom side padding controls. Add settings to adjust left and right padding so content can be condensed toward the middle or expanded to full width. Include padding options like none, small, medium, large, and custom pixel values for precise control over the horizontal spacing of the collapsible content rows., The custom left and right padding do not move the text or content, Add hover animation and opening animation settings, I want to create a heading and subheading above the accordion and also give me the option to add more content containers., I need to be able to add padding above the main heading, and also be able to reduce padding between subheading and content

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-accordion-{{ ai_gen_id }} {
    width: 100%;
    max-width: 100%;
    padding-top: {{ block.settings.section_padding_top }}px;
  }

  .ai-accordion-header-section-{{ ai_gen_id }} {
    text-align: {{ block.settings.header_alignment }};
    margin-bottom: {{ block.settings.header_to_content_spacing }}px;
    {% if block.settings.padding_horizontal == 'custom' %}
      padding-left: {{ block.settings.padding_left_custom }}px;
      padding-right: {{ block.settings.padding_right_custom }}px;
    {% else %}
      {% case block.settings.padding_horizontal %}
        {% when 'none' %}
          padding-left: 0;
          padding-right: 0;
        {% when 'small' %}
          padding-left: 16px;
          padding-right: 16px;
        {% when 'medium' %}
          padding-left: 32px;
          padding-right: 32px;
        {% when 'large' %}
          padding-left: 64px;
          padding-right: 64px;
      {% endcase %}
    {% endif %}
  }

  .ai-accordion-main-heading-{{ ai_gen_id }} {
    font-size: {{ block.settings.main_heading_size }}px;
    font-weight: {{ block.settings.main_heading_weight }};
    color: {{ block.settings.main_heading_color }};
    margin: 0 0 {{ block.settings.heading_gap }}px 0;
    line-height: 1.2;}

  .ai-accordion-subheading-{{ ai_gen_id }} {
    font-size: {{ block.settings.subheading_size }}px;
    font-weight: {{ block.settings.subheading_weight }};
    color: {{ block.settings.subheading_color }};
    margin: 0;
    line-height: 1.4;
  }

  .ai-accordion-container-{{ ai_gen_id }} {
    padding-top: {{ block.settings.padding_vertical }}px;
    padding-bottom: {{ block.settings.padding_vertical }}px;
  }

  .ai-accordion-item-{{ ai_gen_id }} {
    border-bottom: {{ block.settings.border_width }}px solid {{ block.settings.border_color }};
    {% if block.settings.padding_horizontal == 'custom' %}
      padding-left: {{ block.settings.padding_left_custom }}px;
      padding-right: {{ block.settings.padding_right_custom }}px;
    {% else %}
      {% case block.settings.padding_horizontal %}
        {% when 'none' %}
          padding-left: 0;
          padding-right: 0;
        {% when 'small' %}
          padding-left: 16px;
          padding-right: 16px;
        {% when 'medium' %}
          padding-left: 32px;
          padding-right: 32px;
        {% when 'large' %}
          padding-left: 64px;
          padding-right: 64px;
      {% endcase %}
    {% endif %}
    {% if block.settings.hover_animation == 'background' %}
      transition: background-color {{ block.settings.animation_duration }}ms ease;
    {% elsif block.settings.hover_animation == 'scale' %}
      transition: transform {{ block.settings.animation_duration }}ms ease;
    {% elsif block.settings.hover_animation == 'slide' %}
      transition: transform {{ block.settings.animation_duration }}ms ease;
    {% endif %}
  }

  .ai-accordion-item-{{ ai_gen_id }}:first-child {
    border-top: {{ block.settings.border_width }}px solid {{ block.settings.border_color }};
  }

  {% if block.settings.hover_animation == 'background' %}
    .ai-accordion-item-{{ ai_gen_id }}:hover {
      background-color: {{ block.settings.hover_background_color }};
    }
  {% elsif block.settings.hover_animation == 'scale' %}
    .ai-accordion-item-{{ ai_gen_id }}:hover {
      transform: scale(1.02);
    }
  {% elsif block.settings.hover_animation == 'slide' %}
    .ai-accordion-item-{{ ai_gen_id }}:hover {
      transform: translateX({{ block.settings.hover_slide_distance }}px);
    }
  {% endif %}

  .ai-accordion-header-{{ ai_gen_id }} {
    width: 100%;
    background: none;
    border: none;
    padding: {{ block.settings.item_padding }}px 0;
    text-align: left;
    cursor: pointer;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: {{ block.settings.heading_size }}px;
    font-weight: {{ block.settings.heading_weight }};
    color: {{ block.settings.heading_color }};
    transition: color {{ block.settings.animation_duration }}ms ease;}

  .ai-accordion-header-{{ ai_gen_id }}:hover {
    color: {{ block.settings.heading_hover_color }};
  }

  .ai-accordion-header-{{ ai_gen_id }}:focus {
    outline:2px solid {{ block.settings.heading_color }};
    outline-offset: 2px;
  }

  .ai-accordion-icon-{{ ai_gen_id }} {
    width: {{ block.settings.icon_size }}px;
    height: {{ block.settings.icon_size }}px;
    transition: transform {{ block.settings.animation_duration }}ms ease;flex-shrink: 0;
    margin-left: 16px;
  }

  .ai-accordion-header-{{ ai_gen_id }}[aria-expanded="true"] .ai-accordion-icon-{{ ai_gen_id }} {
    {% if block.settings.icon_animation == 'rotate_180' %}
      transform: rotate(180deg);
    {% elsif block.settings.icon_animation == 'rotate_90' %}
      transform: rotate(90deg);
    {% elsif block.settings.icon_animation == 'scale' %}
      transform: scale(1.2);
    {% endif %}
  }

  .ai-accordion-content-{{ ai_gen_id }} {
    overflow: hidden;
    {% if block.settings.opening_animation == 'slide' %}
      transition: max-height {{ block.settings.animation_duration }}ms ease;
      max-height: 0;
    {% elsif block.settings.opening_animation == 'fade' %}
      transition: opacity {{ block.settings.animation_duration }}ms ease, max-height {{ block.settings.animation_duration }}ms ease;
      max-height: 0;
      opacity: 0;
    {% elsif block.settings.opening_animation == 'scale' %}
      transition: transform {{ block.settings.animation_duration }}ms ease, max-height {{ block.settings.animation_duration }}ms ease;
      max-height: 0;
      transform: scaleY(0);transform-origin: top;
    {% else %}
      transition: max-height {{ block.settings.animation_duration }}ms ease;
      max-height: 0;
    {% endif %}
  }

  .ai-accordion-content-{{ ai_gen_id }}.active {
    max-height: 1000px;
    {% if block.settings.opening_animation == 'fade' %}
      opacity: 1;{% elsif block.settings.opening_animation == 'scale' %}
      transform: scaleY(1);
    {% endif %}
  }

  .ai-accordion-content-inner-{{ ai_gen_id }} {
    padding: 0 0 {{ block.settings.item_padding }}px 0;
    color: {{ block.settings.content_color }};
    font-size: {{ block.settings.content_size }}px;
    line-height:1.6;
    {% if block.settings.opening_animation == 'slide_content' %}
      transition: transform {{ block.settings.animation_duration }}ms ease;
      transform: translateY(-10px);
    {% endif %}
  }

  {% if block.settings.opening_animation == 'slide_content' %}
    .ai-accordion-content-{{ ai_gen_id }}.active .ai-accordion-content-inner-{{ ai_gen_id }} {
      transform: translateY(0);
    }
  {% endif %}

  @media screen and (max-width: 749px) {
    .ai-accordion-{{ ai_gen_id }} {
      padding-top: {{ block.settings.section_padding_top | times: 0.7 }}px;
    }

    .ai-accordion-header-section-{{ ai_gen_id }} {
      margin-bottom: {{ block.settings.header_to_content_spacing | times: 0.7 }}px;{% if block.settings.padding_horizontal == 'custom' %}
        padding-left: {{ block.settings.padding_left_custom | times: 0.6 }}px;
        padding-right: {{ block.settings.padding_right_custom | times: 0.6 }}px;
      {% else %}
        {% case block.settings.padding_horizontal %}
          {% when 'none' %}
            padding-left: 0;
            padding-right: 0;
          {% when 'small' %}
            padding-left: 12px;
            padding-right: 12px;
          {% when 'medium' %}
            padding-left: 20px;
            padding-right: 20px;
          {% when 'large' %}
            padding-left: 32px;
            padding-right: 32px;
        {% endcase %}
      {% endif %}
    }

    .ai-accordion-item-{{ ai_gen_id }} {
      {% if block.settings.padding_horizontal == 'custom' %}
        padding-left: {{ block.settings.padding_left_custom | times: 0.6 }}px;
        padding-right: {{ block.settings.padding_right_custom | times: 0.6 }}px;
      {% else %}
        {% case block.settings.padding_horizontal %}
          {% when 'none' %}
            padding-left: 0;
            padding-right: 0;
          {% when 'small' %}
            padding-left: 12px;
            padding-right: 12px;
          {% when 'medium' %}
            padding-left: 20px;
            padding-right: 20px;
          {% when 'large' %}
            padding-left: 32px;
            padding-right: 32px;
        {% endcase %}
      {% endif %}
    }

    .ai-accordion-main-heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.main_heading_size | times: 0.8 }}px;}

    .ai-accordion-subheading-{{ ai_gen_id }} {
      font-size: {{ block.settings.subheading_size | times: 0.9 }}px;
    }

    .ai-accordion-header-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 0.9 }}px;
    }

    .ai-accordion-content-inner-{{ ai_gen_id }} {
      font-size: {{ block.settings.content_size | times: 0.9 }}px;
    }

    {% if block.settings.hover_animation == 'slide' %}
      .ai-accordion-item-{{ ai_gen_id }}:hover {
        transform: translateX({{ block.settings.hover_slide_distance | times: 0.5 }}px);
      }
    {% endif %}
  }
{% endstyle %}

<accordion-{{ ai_gen_id }} class="ai-accordion-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  {% if block.settings.main_heading != blank or block.settings.subheading != blank %}
    <div class="ai-accordion-header-section-{{ ai_gen_id }}">
      {% if block.settings.main_heading != blank %}
        <h2 class="ai-accordion-main-heading-{{ ai_gen_id }}">{{ block.settings.main_heading }}</h2>
      {% endif %}
      {% if block.settings.subheading != blank %}
        <div class="ai-accordion-subheading-{{ ai_gen_id }}">{{ block.settings.subheading }}</div>
      {% endif %}
    </div>
  {% endif %}<div class="ai-accordion-container-{{ ai_gen_id }}">
    {% for i in (1..10) %}
      {% liquid
        assign question_key = 'question_' | append: i
        assign answer_key = 'answer_' | append: i
        assign question = block.settings[question_key]
        assign answer = block.settings[answer_key]
      %}

      {% if question != blank and answer != blank %}
        <div class="ai-accordion-item-{{ ai_gen_id }}">
          <button
            class="ai-accordion-header-{{ ai_gen_id }}"
            aria-expanded="false"
            aria-controls="ai-accordion-content-{{ ai_gen_id }}-{{ i }}"
            data-accordion-trigger="{{ i }}"
          >
            <span>{{ question }}</span>
            {% if block.settings.icon_style == 'chevron' %}
              <svg
                class="ai-accordion-icon-{{ ai_gen_id }}"
                xmlns="http://www.w3.org/2000/svg"
                viewBox="0 0 24 24"
                fill="none"
                stroke="currentColor"
                stroke-width="2"
                stroke-linecap="round"
                stroke-linejoin="round"
              >
                <polyline points="6,9 12,15 18,9"></polyline>
              </svg>
            {% elsif block.settings.icon_style == 'plus' %}
              <svg
                class="ai-accordion-icon-{{ ai_gen_id }}"
                xmlns="http://www.w3.org/2000/svg"
                viewBox="0 0 24 24"
                fill="none"
                stroke="currentColor"
                stroke-width="2"
                stroke-linecap="round"
                stroke-linejoin="round"
              >
                <line x1="12" y1="5" x2="12" y2="19"></line>
                <line x1="5" y1="12" x2="19" y2="12"></line>
              </svg>
            {% elsif block.settings.icon_style == 'arrow' %}
              <svg
                class="ai-accordion-icon-{{ ai_gen_id }}"
                xmlns="http://www.w3.org/2000/svg"
                viewBox="0 0 24 24"
                fill="none"
                stroke="currentColor"
                stroke-width="2"
                stroke-linecap="round"
                stroke-linejoin="round"
              >
                <polyline points="9,18 15,12 9,6"></polyline>
              </svg>
            {% endif %}
          </button>
          <div
            class="ai-accordion-content-{{ ai_gen_id }}"
            id="ai-accordion-content-{{ ai_gen_id }}-{{ i }}"
            data-accordion-content="{{ i }}"
          >
            <div class="ai-accordion-content-inner-{{ ai_gen_id }}">
              {{ answer }}
            </div>
          </div>
        </div>
      {% endif %}
    {% endfor %}
  </div>
</accordion-{{ ai_gen_id }}><script>
  (function() {
    class Accordion{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.triggers = this.querySelectorAll('[data-accordion-trigger]');
        this.contents = this.querySelectorAll('[data-accordion-content]');
        this.setupEventListeners();
      }

      setupEventListeners() {
        this.triggers.forEach((trigger) => {
          trigger.addEventListener('click', () => {
            const targetId = trigger.getAttribute('data-accordion-trigger');
            const content = this.querySelector(`[data-accordion-content="${targetId}"]`);
            const isExpanded = trigger.getAttribute('aria-expanded') === 'true';

            if ({{ block.settings.allow_multiple | json }} === false) {
              this.triggers.forEach((otherTrigger) => {
                if (otherTrigger !== trigger) {
                  otherTrigger.setAttribute('aria-expanded', 'false');const otherId = otherTrigger.getAttribute('data-accordion-trigger');
                  const otherContent = this.querySelector(`[data-accordion-content="${otherId}"]`);
                  otherContent.classList.remove('active');
                }
              });
            }

            trigger.setAttribute('aria-expanded', !isExpanded);
            if (!isExpanded) {
              content.classList.add('active');
            } else {
              content.classList.remove('active');
            }
          });
        });
      }
    }

    customElements.define('accordion-{{ ai_gen_id }}', Accordion{{ ai_gen_id }});
  })();
</script>

